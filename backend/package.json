{
  "name": "backend",
  "version": "0.2.0",
  "description": "Backend for club webstore",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "tsc": "tsc",
    "transpile": "tsc -p tsconfig.json",
    "build": "rm -rf dist/build && cd ../frontend && npm run build --prod && cp -r build ../backend/dist && cd ../frontend && rm -rf build",
    "clean": "rm -rf build",
    "deploy": "cd ../ && git subtree push --prefix backend heroku main",
    "dev": "ts-node-dev src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "logs:prod": "heroku logs --tail",
    "lint": "eslint ."
  },
  "author": "Ville Manninen",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.3.0",
    "mongoose-unique-validator": "^3.0.0",
    "save-dev": "0.0.1-security",
    "tslog": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose-unique-validator": "^1.0.5",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3",
    "winston": "^3.7.2"
  }
}